{
    "schemaVersion": 1,
    "id": "fpsdisplay",
    "version": "${version}",
    "name": "FPS-Display",
    "description": "A simple Fabric mod to show your ingame FPS",
    "authors": [
        "Grayray75"
    ],
    "contact": {
        "homepage": "https://github.com/Grayray75/FPS-Display",
        "sources": "https://github.com/Grayray75/FPS-Display",
        "issues": "https://github.com/Grayray75/FPS-Display/issues"
    },
    "license": "",
    "icon": "assets/fpsdisplay/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "io.grayray75.fabric.fpsdisplay.ClientInit"
        ]
    },
    "mixins": [
        "fpsdisplay.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.9.0",
        "minecraft": "1.15.x"
    },
    "custom": {
        "modmenu:clientsideOnly": true
    }
}